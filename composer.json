{
    "name": "codemonster-ru/annabel-skeleton",
    "description": "Skeleton application for Annabel framework",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "codemonster-ru/annabel": "<1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "start": "@php -S localhost:8000 -t public"
    },
    "config": {
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
